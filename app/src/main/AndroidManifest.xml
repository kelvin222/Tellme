<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.a3nitysoft.kelvin.tellme">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />

    <application
        android:name=".Tellme"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ProfilerActivity"></activity>
        <activity
            android:name=".FeedviewActivity"
            android:parentActivityName=".FeedActivity" />
        <activity
            android:name=".FeedActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".PolicyActivity"
            android:parentActivityName=".AboutActivity" />
        <activity
            android:name=".AboutActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".ContactActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".WeburlActivity"
            android:parentActivityName=".NewsViewActivity" />
        <activity
            android:name=".WebViewerActivity"
            android:parentActivityName=".NewsViewActivity" />
        <activity
            android:name=".AudienceActivity"
            android:parentActivityName=".ReviewViewActivity" />
        <activity
            android:name=".TaggedActivity"
            android:parentActivityName=".ReviewViewActivity" />
        <activity android:name=".LoadingActivity" />
        <activity
            android:name=".CommentsActivity"
            android:parentActivityName=".ReviewViewActivity" />

        <meta-data
            android:name="com.google.android.gms.ads.com.a3nitysoft.kelvin.tellme"
            android:value="ca-app-pub-4481331290406181~1599005010"/>
        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <activity
            android:name=".MainActivity"
            android:launchMode="singleTop">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="com.package.name.MainActivity" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.theartofdev.edmodo.cropper.CropImageActivity"
            android:theme="@style/Base.Theme.AppCompat" />
        <activity android:name=".Login" />
        <activity android:name=".Splash" />
        <activity android:name=".Reg" />
        <activity
            android:name=".Account"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".PostReviewActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".ReviewViewActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".NewsViewActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".PostNewsActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".SearchActivity"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".ProfileActivity" />
    </application>

</manifest>